#include <iostream>
using namespace std;

int main() {
	int T;
	cin>>T;
	while(T--){
	    int D;
	    string N;
	    cin>>D;
	    cin>>N;
	    int flag=0;
	    for(int i=0;i<D;i++){
	        if(N[i]=='0'|| N[i]=='5'){
	            flag=1;
	            break;
	        }
	        
	    }
	    if(flag==1){
	        cout<<"YES"<<endl;
	    }
	    else{
	        cout<<"NO"<<endl;
	    }
	   
	}
	return 0;
}
