#include <iostream>
using namespace std;

int main() {
    int T;
    cin>>T;
    while(T--){
        int  L;
        cin>>L;
        int V1,V2;
        cin>>V1>>V2;
        float dist1=(L/V1);
        float dist2=(L/V2);
        int a,b;
        if(L%V1==0){
            a=L/V1;
    }
    else{
        a=(L/V1)+1;
    }
    if(L%V2==0){
        b=L/V2;
    }
    else{
        b=(L/V2)+1;
    }
    cout<<(a-b-1)<<endl;
    }
	// your code goes here
	return 0;
}
